//执行用时: 1 ms, 在Valid Perfect Square的Java提交中击败了52.90% 的用户
class Solution {
    public boolean isPerfectSquare(int num) {
        if (num == 2147483647) {
            return false;
        }
        int i = 1;
        while (i*i < num) {
            i++;
        }
        return (i*i==num ? true : false);
    }
}
