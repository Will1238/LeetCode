class Solution {
    public boolean containsNearbyDuplicate(int[] nums, int k) {
        for (int i = 1; i <= k; i++) {
            for (int j = 0; j + i < nums.length; j++) {
                if (nums[j] == nums[j + i]) {
                    return true;
                }
            }
        }
        return false;
    }
}
